    <owl:ObjectProperty rdf:about="&pslo;perform">
        <rdfs:comment>What is performed by the technicians. Here the services.</rdfs:comment>
        <rdfs:domain rdf:resource="&pslo;actor"/>
        <rdfs:range rdf:resource="&pslo;service"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/pslo.owl#performed_by -->

    <owl:ObjectProperty rdf:about="&pslo;performed_by">
        <rdfs:comment>By whom the service is performed. Here the technicians.</rdfs:comment>
        <rdfs:range rdf:resource="&pslo;actor"/>
        <owl:inverseOf rdf:resource="&pslo;perform"/>
        <rdfs:domain rdf:resource="&pslo;service"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/pslo.owl#performed_for -->

    <owl:ObjectProperty rdf:about="&pslo;performed_for">
        <rdfs:comment>For whom the service is performed. Here the clients.</rdfs:comment>
        <rdfs:range rdf:resource="&pslo;client"/>
        <rdfs:domain rdf:resource="&pslo;service"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/pslo.owl#performed_in -->

    <owl:ObjectProperty rdf:about="&pslo;performed_in">
        <rdfs:comment>Where the service is performed. Here the centers.</rdfs:comment>
        <rdfs:range rdf:resource="&pslo;maintenance_center"/>
        <rdfs:domain rdf:resource="&pslo;service"/>
