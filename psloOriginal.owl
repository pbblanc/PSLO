        <rdfs:domain rdf:resource="&pslo;car"/>
        <rdfs:range rdf:resource="&pslo;structure"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/pslo.owl#employ -->

    <owl:ObjectProperty rdf:about="&pslo;employ">
        <rdfs:comment>Who is employed by a company (i.e. center). Here technicians.</rdfs:comment>
        <rdfs:domain rdf:resource="&pslo;battery_recycling_center"/>
        <rdfs:range rdf:resource="&pslo;battery_recycling_center_technician"/>
        <rdfs:domain rdf:resource="&pslo;battery_store_center"/>
