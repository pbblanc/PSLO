    <!-- http://example.org/pslo.owl#compose -->

    <owl:ObjectProperty rdf:about="&pslo;compose">
        <rdfs:comment>What makes the car. Here a battery or a structure.</rdfs:comment>
        <rdfs:domain rdf:resource="&pslo;battery"/>
        <rdfs:range rdf:resource="&pslo;car"/>
        <owl:inverseOf rdf:resource="&pslo;composed_by"/>
        <rdfs:domain rdf:resource="&pslo;structure"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/pslo.owl#composed_by -->

    <owl:ObjectProperty rdf:about="&pslo;composed_by">
        <rdfs:comment>Into what is embedded the the battery or the structure. Here a car.</rdfs:comment>
