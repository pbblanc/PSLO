    </owl:ObjectProperty>
    


    <!-- http://example.org/pslo.owl#manufacture -->

    <owl:ObjectProperty rdf:about="&pslo;manufacture">
        <rdfs:comment>What is manufactured by the product provider. Here a battery or a structure.

This Object Property is not used but would be if we add manufacturers to the model.</rdfs:comment>
        <rdfs:domain rdf:resource="&pslo;actor"/>
        <rdfs:range rdf:resource="&pslo;car"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/pslo.owl#manufactured_by -->

    <owl:ObjectProperty rdf:about="&pslo;manufactured_by">
        <rdfs:comment>Who manufactured the parts of the car (battery and structure). Here a product provider.

This Object Property is not used but would be if we add manufacturers to the model.</rdfs:comment>
        <rdfs:range rdf:resource="&pslo;actor"/>
        <rdfs:domain rdf:resource="&pslo;car"/>
        <owl:inverseOf rdf:resource="&pslo;manufacture"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/pslo.owl#own -->

    <owl:ObjectProperty rdf:about="&pslo;own">
        <rdfs:comment>What the client owns. Here a car.</rdfs:comment>
        <rdfs:range rdf:resource="&pslo;car"/>
        <rdfs:domain rdf:resource="&pslo;client"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/pslo.owl#owned_by -->

    <owl:ObjectProperty rdf:about="&pslo;owned_by">
        <rdfs:comment>By whom is owned the car. Here a client.</rdfs:comment>
        <rdfs:domain rdf:resource="&pslo;car"/>
        <rdfs:range rdf:resource="&pslo;client"/>
        <owl:inverseOf rdf:resource="&pslo;own"/>
    </owl:ObjectProperty>
    


