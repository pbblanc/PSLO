    <!-- http://example.org/pslo.owl#performed_on -->

    <owl:ObjectProperty rdf:about="&pslo;performed_on">
        <rdfs:comment>On what the service is performed. Here usually on the battery or the structure.</rdfs:comment>
        <rdfs:range rdf:resource="&pslo;car"/>
        <rdfs:domain rdf:resource="&pslo;service"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/pslo.owl#recycled_by -->

    <owl:ObjectProperty rdf:about="&pslo;recycled_by">
        <rdfs:comment>The service that did recycle the battery. Here the battery recycling service.

This Object Property is not really needed since we can retrieve the same information and create the same link using the Object Property &quot;performed_on&quot; and limiting the search to battery recycling services. We keep it though to make things easier to understand.</rdfs:comment>
        <rdfs:domain rdf:resource="&pslo;battery"/>
        <rdfs:range rdf:resource="&pslo;battery_recycling"/>
    </owl:ObjectProperty>
    


