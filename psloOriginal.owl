        <rdfs:domain rdf:resource="&pslo;maintenance_center"/>
        <rdfs:range rdf:resource="&pslo;maintenance_center_technician"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/pslo.owl#employed_by -->

    <owl:ObjectProperty rdf:about="&pslo;employed_by">
