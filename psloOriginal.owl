        <rdfs:domain rdf:resource="&pslo;battery_recycling_center_technician"/>
        <rdfs:range rdf:resource="&pslo;battery_store_center"/>
        <rdfs:range rdf:resource="&pslo;maintenance_center"/>
        <rdfs:domain rdf:resource="&pslo;maintenance_center_technician"/>
    </owl:ObjectProperty>
    


